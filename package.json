{
  "name": "storybook-addon-mock",
  "description": "A monorepo for storybook-addon-mock",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn workspace storybook-addon-mock run build",
    "storybook": "yarn workspace storybook-addon-mock-docs run storybook",
    "lint": "yarn workspaces run lint",
    "lint:fix": "yarn workspaces run lint:fix",
    "test": "yarn workspaces run test",
    "ci": "yarn run lint && yarn run test && yarn run build",
    "build-storybook": "yarn workspace storybook-addon-mock-docs run build-storybook",
    "serve-storybook": "yarn workspace storybook-addon-mock-docs run serve-storybook",
    "dedup": "yarn-deduplicate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nutboltu/storybook-addon-mock.git"
  },
  "keywords": [
    "storybook",
    "mock",
    "http-mock",
    "fetch-mock",
    "xhr-mock",
    "mock-data",
    "storybook-addon",
    "data-state",
    "storybook-mock-request",
    "mock-request"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "prop-types": "^15.8.1",
    "webpack": "^5.72.1",
    "yarn-deduplicate": "^5.0.0"
  }
}
