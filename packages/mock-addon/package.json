{
  "name": "storybook-addon-mock",
  "version": "3.2.0",
  "description": "A storybook addon to mock fetch/XHR request",
  "keywords": [
    "storybook",
    "mock",
    "http-mock",
    "fetch-mock",
    "xhr-mock",
    "mock-data",
    "storybook-addon",
    "data-state",
    "storybook-mock-request",
    "mock-request"
  ],
  "author": "Farhad Yasir (nutboltu)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nutboltu/storybook-addon-mock.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/ts/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "buildBabel": "concurrently \"yarn buildBabel:cjs\" \"yarn buildBabel:esm\"",
    "buildBabel:cjs": "babel ./src -d ./dist/cjs --extensions \".js,.jsx,.ts,.tsx\"",
    "buildBabel:esm": "babel ./src -d ./dist/esm --env-name esm --extensions \".js,.jsx,.ts,.tsx\"",
    "prebuild": "yarn clean",
    "build": "yarn buildBabel",
    "build:watch": "yarn buildBabel:esm -- --watch",
    "lint": "eslint .",
    "lint:fix": "yarn run lint --fix",
    "test": "jest src",
    "prerelease": "zx scripts/prepublish-checks.mjs",
    "release": "yarn build && auto shipit"
  },
  "dependencies": {
    "mock-xmlhttprequest": "^8.1.0",
    "path-to-regexp": "^6.2.0",
    "polished": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.13.0",
    "auto": "^10.3.0",
    "boxen": "^5.0.1",
    "concurrently": "^6.2.0",
    "dedent": "^0.7.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4",
    "zx": "^1.14.1"
  },
  "peerDependencies": {
    "@storybook/addons": "^6.4.0",
    "@storybook/api": "^6.4.0",
    "@storybook/components": "^6.4.0",
    "@storybook/theming": "^6.4.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "storybook": {
    "displayName": "Mock API Request",
    "supportedFrameworks": [
      "react",
      "vue"
    ],
    "icon": "https://raw.githubusercontent.com/nutboltu/storybook-addon-mock/master/assets/logo.svg"
  }
}
